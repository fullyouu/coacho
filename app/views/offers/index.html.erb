<div class="container">

  <div class="header">
    <h1>Offers</h1>
  </div>

  <div class="cards">
    <% @offers.each do |offer| %>
      <%= link_to offer.title, offer_path(offer) %>
    <% end %>
  </div>

  <div class="link">
    <p><%= link_to 'Create a new offer', new_offer_path(@offer) %></p>
  </div>
</div>
